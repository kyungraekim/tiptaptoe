{
  "name": "@tiptaptoe/ui-components",
  "version": "1.0.0",
  "description": "Reusable UI components and icons for TipTapToe editor",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "icons",
    "tiptap",
    "editor"
  ],
  "author": "TipTapToe",
  "license": "MIT"
}